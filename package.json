{
  "name": "barklarm-mobile",
  "main": "expo-router/entry",
  "version": "0.1.2",
  "scripts": {
    "start": "expo start",
    "start:tunnel": "expo start --tunnel",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --passWithNoTests --runInBand",
    "test:cov": "jest --passWithNoTests --coverage --runInBand",
    "test:dev": "jest --watch --runInBand",
    "tsc:check": "tsc --noEmit --skipLibCheck",
    "lint:check": "eslint src --ext .ts,.tsx --max-warnings=0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format:fix": "prettier --write .",
    "format:check": "prettier --check .",
    "lint-staged": "lint-staged",
    "lint": "expo lint",
    "prepare": "husky",
    "commit": "git add . && cz",
    "eas": "eas",
    "release:patch": "yarn version --patch && git push",
    "release:minor": "yarn version --patch && git push"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@octokit/rest": "^19.0.4",
    "@react-native-picker/picker": "^2.7.7",
    "@react-navigation/native": "^6.0.2",
    "azure-devops-node-api": "^14.0.2",
    "expo": "~51.0.28",
    "expo-background-fetch": "~12.0.1",
    "expo-constants": "~16.0.2",
    "expo-font": "~12.0.9",
    "expo-linking": "~6.3.1",
    "expo-notifications": "~0.28.16",
    "expo-router": "~3.5.23",
    "expo-secure-store": "~13.0.2",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "fast-xml-parser": "^4.4.1",
    "opsgenie-sdk": "^0.5.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-paper": "^5.12.5",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-vector-icons": "^10.1.0",
    "react-native-web": "~0.19.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@faker-js/faker": "^8.4.1",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "husky": "^9.1.5",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.3",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
